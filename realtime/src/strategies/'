import sqlite3
from pprint import pprint

import settings


class BacktestPipeline:

    def __init__(self):

        iex_db = sqlite3.connect(settings.DATA_DIRECTORY / settings.IEX_DATABASE_NAME)

        days = iex_db.execute('SELECT * FROM iex_days;').fetchall()

        pprint(days)


foo = BacktestPipeline()
